{
  "name": "app-with-express-no-stress",
  "version": "1.0.0",
  "description": "App with express-no-stress",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts",
    "lint": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet",
    "lint:fix": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet --fix",
    "test": "mocha test/**/*.ts --exit",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "winston": "^3.2.1",
    "uuid": "^3.3.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-list-middleware": "^1.0.1",
    "cors": "^2.8.5",
    "ws": "^7.2.1",
    "redis": "^2.8.0",
    "connect-redis": "^4.0.3",
    "swagger-express-middleware": "^2.0.4"
  },
  "devDependencies": {
    "@types/winston": "^2.4.4",
    "@types/uuid": "^3.4.6",
    "@types/chai": "^4.2.4",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/mocha": "^5.2.7",
    "@types/cors": "^2.8.6",
    "@types/ws": "^6.0.4",
    "@types/redis": "^2.8.14",
    "@types/connect-redis": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "chai": "^4.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)"
}
